{
  "name": "@agentlab/rjsf-antd",
  "version": "0.1.9",
  "license": "MIT",
  "description": "Ant Design theme, fields and widgets for react-jsonschema-form",
  "bugs": {
    "url": "https://github.com/agentlab/rjsf-antd/issues"
  },
  "homepage": "https://github.com/agentlab/rjsf-antd#readme",
  "author": "amivanoff",
  "keywords": [
    "Ant Design",
    "react-jsonschema-form",
    "jsonschema",
    "json-schema",
    "json",
    "schema",
    "form",
    "react",
    "antd",
    "rjsf-antd"
  ],
  "main": "lib/index.js",
  "types": "lib",
  "files": [
    "lib",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/agentlab/rjsf-antd.git"
  },
  "dependencies": {},
  "scripts": {
    "start": "react-scripts start",
    "build": "rm -rf lib && tsc -p ./publish.tsconfig.json",
    "test": "react-scripts test --verbose=true",
    "test2": "react-scripts test --no-watch",
    "test-debug": "react-scripts --inspect-brk=0.0.0.0:9229 test --env=jsdom --runInBand -i",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 9009 -s ./public -c .storybook",
    "build-storybook": "build-storybook -s ./public",
    "format": "prettier-eslint --write \"{,!(node_modules)/**/}*.{js,jsx,ts,tsx}\""
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "peerDependencies": {
    "antd": ">=3.23.6",
    "react": ">=16.10.2",
    "react-jsonschema-form": ">=1.8.0"
  },
  "devDependencies": {
