    "@storybook/addon-storyshots": "^5.2.4",
    "@storybook/addons": "^5.2.4",
    "@storybook/client-logger": "^5.2.4",
    "@storybook/react": "^5.2.4",
    "@types/classnames": "^2.2.9",
    "@types/jest": "^24.0.19",
